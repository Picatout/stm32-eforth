 133              	*	Assemble inline direct threaded code ending.
 134              	***********************************************/
 135              		.macro _CALL fn /* low level routine call */ 
 136              	 	PUSH {LR}
 137              		BL \fn  
 138              		POP {LR}
 139              		.endm
